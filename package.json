{
  "name": "chefcam.",
  "version": "0.0.1",
  "description": "Find recipes from just a photo of the ingredients with AI.",
  "author": "mst-mkt, imoken777, rarandeyo, sorainiad",
  "scripts": {
    "dev": "turbo dev",
    "dev:frontend": "pnpm -F frontend run dev",
    "dev:backend": "pnpm -F backend run dev",
    "check": "turbo check",
    "fix": "turbo fix",
    "typecheck": "turbo typecheck",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "biome-config": "workspace:*",
    "husky": "^9.1.5",
    "lint-staged": "^15.2.10",
    "turbo": "^2.1.1"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,css}": [
      "biome check --write"
    ]
  },
  "volta": {
    "node": "20.14.0"
  },
  "packageManager": "pnpm@9.3.0"
}
